            }

        });
        $(document).on("click", ".bi-x-circle-fill", function () {
            $(this).parent().remove();
            $(".add-more-img-btn").prop('disabled', false);
        });
        $("#modalSubmitButton").click(function () {
            var tdId = $("#hdntdId").val();
