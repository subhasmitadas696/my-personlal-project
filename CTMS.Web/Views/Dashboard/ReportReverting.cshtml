    function ReportValidation() {
        var liCountWithoutCross = $(".uploaded-img-list li:not(:has(.bi-x-circle-fill))").length;
        var videoLiCountWithoutCross = $(".uploaded-video-list li:not(:has(.bi-x-circle-fill))").length;
        if (liCountWithoutCross === 9 && videoLiCountWithoutCross === 3) {
            if ($("#remark").val().trim() != "") {
                $('#remark').removeClass("is-invalid");
                $('#errtxtRemark').text("");
                return true;
            }
            else {
                $('#remark').addClass("form-control shadow-sm is-invalid");
                $('#errtxtRemark').text("Please Enter Remark!");
                $('#remark').focus();
                return false;
            }
        }
        else {
            new swal("", "Please Upload All The Photo and Video!", "");
            return false;
        }
    }
    $("#submitButton").click(function () {
