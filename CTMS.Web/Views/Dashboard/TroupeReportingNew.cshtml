@{
    ViewData["Title"] = "Reporting";
    Layout = "~/Views/Shared/_LayoutPublic.cshtml";
    var Data = ViewBag.TroupeDetails;
}
@Html.AntiForgeryToken()
<link href="~/css/troupe-reporting-new.css" rel="stylesheet" />
<style>

</style>
<main class="content-body">
    <div class="card card--lg">
        @if (Data != null)
        {
            <div class="card-body">
                <h5 class="view-form-details__title">Event Details</h5>
                <div class="row g-4 mb-4">
                    <div class="col-md-4">
                        <input type="hidden" id="hdntroupeid" value="@Data[0].TroupeId" />
                        <input type="hidden" id="hdnassigneventid" value="@Data[0].AssignEventId" />
                        <input type="hidden" id="hdnimgid" value="" />
                        <input type="hidden" id="hdnDynImgIdName" value="" />
                        <input type="hidden" id="hdnvdoid" value="" />
                        <input type="hidden" id="hdnmemcheckid" value="" />
                        <input type="hidden" id="hdntdId" value="" />
                        <input type="hidden" id="hdnmemId" value="" />
                        <input type="hidden" id="hdnvillageid" value="" />
                        <input type="hidden" id="hdnVdovillageid" value="" />
                        <input type="hidden" id="hdnflagcount" value=0 />
                        <input type="hidden" id="hdncrossimgvalue" value=0 />
                        <label for="" class="control-label">
                            Event Title
                        </label>
                        <strong>
                            @Data[0].EventTitle
                        </strong>
                    </div>
                    <div class="col-md-4">
                        <label for="" class="control-label">
                            Event Date
                        </label>
                        <strong>
                            @Data[0].DateOfPerform
                        </strong>
                    </div>
                    <div class="col-md-4">
                        <label for="" class="control-label">
                            Address
                        </label>
                        <strong>
                            @Data[0].DistrictName
                        </strong>
                        <div class="text-muted">@Data[0].BlockName</div>
                        <div class="text-muted">@Data[0].GPName</div>

                    </div>
                </div>
                <h5 class="view-form-details__title">Place of Perfromance</h5>
                <div>
                    <strong>Note:</strong>
                    <ul class="note-list">
                        <li>Please Upload 3 Photos and 1 Video For Each Village.</li>
                        <li>Add Member Perform Count.</li>
                        <li>After Adding 3 Photos Click On The Upload Button and Same For Video.</li>
                        <li>Maximum Image Size 1MB and Video Size 5MB.</li>
                    </ul>
                </div>
                @{
                    int count = 1;
                }
                @foreach (var items in Data)
                {
                    <div class="place-box">
                        <div class="p-3 memberdetails">
                            <div class="row g-4">
                                <div class="col-md-6 col-xl-3 d-none dnonevlgid">
                                    <label for="" class="control-label">
                                        Village Id
                                    </label>
                                    <strong>@items.VillageId</strong>
                                </div>
                                <div class="col-md-6 col-xl-3">
                                    <label for="" class="control-label">
                                        Village Name
                                    </label>
                                    <strong>@items.VLGNAME</strong>
                                </div>
                                <div class="col-md-6 col-xl-3">
                                    <label for="" class="control-label">
                                        Start-End Time
                                    </label>
                                    <strong>@items.StartTime - @items.EndTime</strong>
                                </div>
                                <div class="col-md-6 col-xl-3">
                                    <label for="" class="control-label">
                                        Authority Details
                                    </label>
                                    <strong>@items.AssignOfficerName, @items.AssignOfficerMobNo</strong>
                                </div>
                                <div class="col-md-6 col-xl-3 text-end">
                                    @if (@count > 1)
                                    {
                                        <input type="hidden" id="hdemmemberid-@count" value="" />
                                        <button type="button" class="btn btn-secondary rounded-pill btn-sm add-more-member-btn" data-td-id="@count" id="addMoreMemberBtn-@count" data-bs-target="#memberListModal" data-bs-toggle="modal" onclick="MemberDetails(@items.TroupeId,'Memcheck_@count')" disabled>
                                            <i class="bi bi-plus"></i> Add Member
                                        </button>
                                        <div class="mt-2 d-flex align-items-center justify-content-end">
                                            <span class="badge text-bg-primary checkedCountDisplay countDisplay-@count me-1" data-td-id="@count">0</span>
                                            <span>Member added</span>
                                        </div>
                                    }
                                    else
                                    {
                                        <input type="hidden" id="hdemmemberid-@count" value="" />
                                        <button type="button" class="btn btn-secondary rounded-pill btn-sm add-more-member-btn" data-td-id="@count" id="addMoreMemberBtn-@count" data-bs-target="#memberListModal" data-bs-toggle="modal" onclick="MemberDetails(@items.TroupeId,'Memcheck_@count')">
                                            <i class="bi bi-plus"></i> Add Member
                                        </button>
                                        <div class="mt-2 d-flex align-items-center justify-content-end">
                                            <span class="badge text-bg-primary checkedCountDisplay countDisplay-@count me-1" data-td-id="@count">0</span>
                                            <span>Member added</span>
                                        </div>
                                    }

                                </div>
                            </div>
                        </div>
                        <div class="hr-divider"></div>
                        <div class="p-3">
                            <div class="row g-4">
                                <div class="col-xl-6 imgvlgid">
                                    <strong>Upload Image <span class="text-danger">*</span></strong>
                                    <div class="place-box__img">
                                        @if (@count > 1)
                                        {
                                            <label class="place-box__img-uploader">
                                                <input type="file" name="name" value="" class="image-upload-input" id="imageUpload-@count" accept="image/*" disabled />
                                                <i class="bi bi-cloud-arrow-up"></i>
                                                <span class="add-more-img-btn" id="addMoreImgBtn" data-folder-id="@items.ImgFolderID" data-village-id="@items.VillageId" data-td-id="@count">
                                                    Drag and drop here
                                                    <strong class="text-primary">Browse File</strong>
                                                </span>
                                            </label>
                                        }
                                        else
                                        {
                                            <label class="place-box__img-uploader">
                                                <input type="file" name="name" value="" class="image-upload-input" id="imageUpload-@count" accept="image/*" />
                                                <i class="bi bi-cloud-arrow-up"></i>
                                                <span class="add-more-img-btn" id="addMoreImgBtn" data-folder-id="@items.ImgFolderID" data-village-id="@items.VillageId" data-td-id="@count">
                                                    Drag and drop here
                                                    <strong class="text-primary">Browse File</strong>
                                                </span>
                                            </label>
                                        }
                                        <ul class="uploaded-img-list" id="imageList-@count">
                                        </ul>
                                    </div>
                                    <small class="text-danger" id="errgrpphoto-@count"></small>
                                    <br/>
                                    @if (@count > 1)
                                    {
                                        <button class="btn btn-primary btn-sm mt-3 add-more-img-btn" id="ImgUpldbtn-@count" disabled>Upload</button>
                                    }
                                    else
                                    {
                                        <button class="btn btn-primary btn-sm mt-3 add-more-img-btn" id="ImgUpldbtn-@count">Upload</button>
                                    }

                                </div>
                                <div class="col-xl-6 vdovlgid">
                                    <strong>Upload Video <span class="text-danger">*</span></strong>
                                    <div class="place-box__img vdotdid">
                                        @if (@count > 1)
                                        {
                                            <label class="place-box__img-uploader">
                                                <input type="file" name="videoUpload" value="" class="video-upload-input" id="videoUpload-@count" accept="video/*" disabled/>
                                                <i class="bi bi-cloud-arrow-up"></i>
                                                <span class="add-more-vdo-btn" data-folder-id="@items.ImgFolderID" data-village-id="@items.VillageId" data-td-id="@count" id="addMorevideoBtn">
                                                    Drag and drop here
                                                    <strong class="text-primary">Browse File</strong>
                                                </span>

                                            </label>
                                        }
                                        else
                                        {
                                            <label class="place-box__img-uploader">
                                                <input type="file" name="videoUpload" value="" class="video-upload-input" id="videoUpload-@count" accept="video/*"/>
                                                <i class="bi bi-cloud-arrow-up"></i>
                                                <span class="add-more-vdo-btn" data-folder-id="@items.ImgFolderID" data-village-id="@items.VillageId" data-td-id="@count" id="addMorevideoBtn">
                                                    Drag and drop here
                                                    <strong class="text-primary">Browse File</strong>
                                                </span>

                                            </label>
                                        }
                                        <ul class="uploaded-img-list uploaded-video-list" id="videoList-@count">
                                        </ul>
                                    </div>
                                    <small class="text-danger" id="errgrpvideo-@count"></small>
                                    <br />
                                    @if (@count > 1)
                                    {
                                        <button class="btn btn-primary btn-sm mt-3 add-more-vdo-btn" id="VdoUpldbtn-@count" disabled>Upload</button>
                                    }
                                    else
                                    {
                                        <button class="btn btn-primary btn-sm mt-3 add-more-vdo-btn" id="VdoUpldbtn-@count">Upload</button>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    count++;
                }
                <div class="row g-4 mb-4">
                    <div class="col-md-12">
                        <div class="form-floating">
                            <textarea class="form-control" placeholder=" " rows="4" id="remark"></textarea>
                            <label for="remark">Remark<span class="text-danger">*</span></label>
                            <small class="text-danger" id="errtxtRemark"></small>
                        </div>
                    </div>
                    <div>
                        <button class="rounded-pill btn btn-primary btn-lg btn--animation" id="submitButton">Submit</button>
                        <button class="rounded-pill btn btn-secondary btn-lg btn--animation">Cancel</button>
                    </div>
                </div>
            </div>
            
        }
    </div>
</main>
<div id="memberListModal" class="modal customodal fade" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-body" id="viewdiv">
                <div class="mb-3 d-flex justify-content-between align-items-center border-bottom pb-2">
                    <h5 class="mb-0 text-primary">@Localizer["Member Details"]</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"><i class="bi bi-x-lg"></i></button>
                </div>
                <ul class="member-list" id="memlist">
                </ul>
                <button type="button" class="btn btn-primary" id="modalSubmitButton">Save</button>
            </div>
        </div>
    </div>
</div>
